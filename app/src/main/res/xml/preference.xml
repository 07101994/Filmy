<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android">

    <SwitchPreference
        android:defaultValue="true"
        android:key="imagequality"
        android:summaryOff="Low"
        android:summaryOn="High"
        android:title="Image Quality" />

    <Preference
        android:enabled="true"
        android:key="license"
        android:selectable="true"
        android:summary=""
        android:title="License" />

    <Preference
        android:enabled="true"
        android:key="Version"
        android:selectable="true"
        android:summary="0.0.1"
        android:title="Version" />

    <Preference
        android:enabled="true"
        android:key="About"
        android:selectable="true"
        android:summary="Made for Cinephiles"
        android:title="About" />

</PreferenceScreen>